def ispalindrome(s):
    n=len(s)
    for i in range(n):
        if (s[i] != s[n-i-1]):
            return False
    return True
def string1(s):
    cnt = 0
    flag = 0
    while(len(s) > 0): 
        if(ispalindrome(s)): 
            flag = 1
            break
        else: 
            cnt += 1
            s = s[:-1] 
    if(flag): 
        print(cnt) 
st="AACECAAAA"
string1(st)
