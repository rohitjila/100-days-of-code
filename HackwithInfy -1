# A number is perfect or not excluding the number itself

def array(n):
    arr=[]
    for i in range(1,n):
        if (n % i == 0):
            arr.append(i)
    b=sum(arr)
    if b == n:
        return ("YES")
    else:
        return ("NO")
for i in range(int(input())):
    n=int(input())
    print(array(n))
