#Codeforces Nastia and Nearly good Numbers
def solve(a,b):
    if (b != 1):
        print("YES")
        print(a*b,a,a*(b+1),end=" ")
    else:
        print("NO")
t=int(input())
for i in range(t):
    a,b=map(int,input().split())
    solve(a,b)
    
    
#k1(a*b) + k2(a) = k3(a)
#x+y=z
#x(a*b) + y(a) = z(a)
#x(b) + y = z
#b + 1 = z
#z = 1 + b
#ab + a = a(b+1)
