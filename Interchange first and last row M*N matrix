def maths(arr,n,m):
    for i in range(m):
        t=arr[0][i]
        arr[0][i]=arr[n-1][i]
        arr[n-1][i]=t
def printlist(arr,n,m):
    for i in range(n):
        for j in range(m):
            print(str(arr[i][j]),end=" ")
arr=[[1,2],
[4,5],
[7,8]]
n=len(arr)
m=len(arr[0])
maths(arr,n,m)
printlist(arr,n,m)
