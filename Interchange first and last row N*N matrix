def maths(arr,n):
    for i in range(n):
        t=arr[0][i]
        arr[0][i]=arr[n-1][i]
        arr[n-1][i]=t
def printlist(arr,n):
    for i in range(n):
        for j in range(n):
            print(str(arr[i][j]),end=" ")
arr=[[1,2,3],
[4,5,6],
[7,8,9]]
n=len(arr[0])
maths(arr,n)
printlist(arr,n)
