def findCommon(ar1, ar2, ar3, n1, n2, n3): 
	i,j,k=0,0,0
	while (i < n1 and j < n2 and k < n3):
		if (ar1[i] == ar2[j] and ar2[j] ==  ar3[k]):
		    print(ar1[i],end=" ")
		    i+=1
		    j+=1
		    k+=1
		elif ar1[i] < ar2[j]: 
			i += 1
		elif ar2[j] < ar3[k]: 
			j += 1
		else: 
			k += 1
ar1 = [1,1,3] 
ar2 = [1,2,3] 
ar3 = [2,3,3] 
n1 = len(ar1) 
n2 = len(ar2) 
n3 = len(ar3) 
findCommon(ar1, ar2, ar3, n1, n2, n3) 
