def array(arr1,arr2,n,m):
    s=set()
    for i in range(n):
        s.add(arr1[i])
    for i in range(m):
        s.add(arr2[i])
    print(len(s))
t=int(input())
for i in range(t):
    n,m=map(int,input().split())
    arr1=list(map(int,input().split()))
    arr2=list(map(int,input().split()))
    array(arr1,arr2,n,m)
