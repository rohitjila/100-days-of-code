def maths(arr,n):
    for i in range(n):
        t=arr[i][0]
        arr[i][0]=arr[i][n-1]
        arr[i][n-1]=t
def printlist(arr,n):
    for i in range(n):
        for j in range(n):
            print(str(arr[i][j]),end=" ")
arr=[[1,2,3],
[4,5,6],
[7,8,9]]
n=len(arr[0])
maths(arr,n)
printlist(arr,n)
